<!DOCTYPE web-app PUBLIC
  "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <context-param>
      <param-name>applicationName</param-name>
      <param-value>Capacity building system</param-value>
  </context-param>

  <servlet>
    <servlet-name>capacityBuildingAuth</servlet-name>
    <servlet-class>com.sys.capacitybuilding.Auth</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>capacityBuildingAuth</servlet-name>
    <url-pattern>/auth</url-pattern>
  </servlet-mapping>
</web-app>
