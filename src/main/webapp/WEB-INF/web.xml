<!DOCTYPE web-app PUBLIC
  "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
  "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>

  <context-param>
      <param-name>appName</param-name>
      <param-value>Capacity building system</param-value>
  </context-param>

  <servlet>
    <servlet-name>capacityBuildingAuth</servlet-name>
    <servlet-class>com.sys.capacitybuilding.Auth</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>capacityBuildingLogin</servlet-name>
    <servlet-class>com.sys.capacitybuilding.Login</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>capacityBuildingHome</servlet-name>
    <servlet-class>com.sys.capacitybuilding.Home</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>capacityBuildingRegister</servlet-name>
    <servlet-class>com.sys.capacitybuilding.Register</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>capacityBuildingAuth</servlet-name>
    <url-pattern>/auth</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>capacityBuildingRegister</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>capacityBuildingLogin</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>capacityBuildingHome</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
</web-app>
